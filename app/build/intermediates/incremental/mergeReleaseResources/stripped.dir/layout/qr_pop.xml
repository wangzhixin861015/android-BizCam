<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ImageView
        android:id="@+id/cameral_layout_rl"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
    <RelativeLayout
        android:id="@+id/qrcode_content"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_280"
        android:background="#1D1E1F"
        android:layout_alignParentBottom="true"
       >
        <ImageView
            android:layout_above="@+id/tv_qr_returun"
            android:id="@+id/iv_qr_return"
            android:src="@drawable/close4"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="@dimen/dp_50"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/tv_qr_returun"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/qr_bottom"
            style="@style/FontWhite14"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true"
            />
    </RelativeLayout>
</RelativeLayout>